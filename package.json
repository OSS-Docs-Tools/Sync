{
  "version": "0.2.8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "docs-sync": "bin/docs-sync"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx test"
    }
  },
  "prettier": {
    "printWidth": 140,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "@oss-docs/sync",
  "author": "github@orta.io",
  "module": "dist/oss-docs-sync.esm.js",
  "size-limit": [
    {
      "path": "dist/oss-docs-sync.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/oss-docs-sync.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/tar-fs": "^2.0.0",
    "@types/yargs": "^15.0.12",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cachedir": "^2.3.0",
    "cross-spawn": "^7.0.1",
    "gunzip-maybe": "^1.4.1",
    "mvdir": "^1.0.21",
    "tar-fs": "^2.1.0",
    "yargs": "^16.2.0"
  }
}
